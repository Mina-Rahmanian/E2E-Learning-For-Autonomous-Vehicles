def path_leaf(path):
  head, tail = ntpath.split(path)
  return tail

data_dir['center'] = data_dir['center'].apply(path_leaf)
data_dir['left'] = data_dir['left'].apply(path_leaf)
data_dir['right'] = data_dir['right'].apply(path_leaf)
